<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>custlogin</servlet-name>
        <servlet-class>com.concert.controller.custlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.concert.controller.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.concert.controller.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookinguser</servlet-name>
        <servlet-class>com.concert.controller.bookinguser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminlogin</servlet-name>
        <servlet-class>com.concert.controller.adminlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminregister</servlet-name>
        <servlet-class>com.concert.controller.adminregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addconcert</servlet-name>
        <servlet-class>com.concert.controller.addconcert</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>concertupdate</servlet-name>
        <servlet-class>com.concert.controller.concertupdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>custlogin</servlet-name>
        <url-pattern>/custlogin</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookinguser</servlet-name>
        <url-pattern>/bookinguser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminlogin</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>adminregister</servlet-name>
        <url-pattern>/adminregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addconcert</servlet-name>
        <url-pattern>/addconcert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>concertupdate</servlet-name>
        <url-pattern>/concertupdate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <resource-ref>
        <res-ref-name>jdbc/myDatasource</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
        <res-sharing-scope>Shareable</res-sharing-scope>
    </resource-ref>
</web-app>
